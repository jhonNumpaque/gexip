<%= simple_form_for @unidad_tiempo, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.error_notification %>

  <%= f.input :nombre %>
  <%= f.input :simbolo %>
  <%= f.input :minutos %>


  <div class="form-actions">
    <%= f.button :submit, 'Guardar', :class => 'btn btn-success' %>
    <%= link_to 'Cancelar', unidades_tiempos_path, :class => "btn" %>
  </div>
<% end %>

  <div class="page-header">
    <h1>Expedientes Terminados<small> por procedimientos</small></h1>
  </div>
    <p><%= link_to 'volver', tiempos_demorados_path %></p>

<% if @tiempos_demorados.present? %>
    <table class="table table-striped table-bordered table-condensed">
      <tr>
        <th>Expediente</th>
        <th>Registrado</th>
        <th>Tiempo de Proceso</th>
        <th>Tiempo Estimado</th>
        <th>Tiempo de Demora(+/-)</th>
        <th>Funcionario</th>
      </tr>
      <% @tiempos_demorados.each do |tiempo_demorado| %>
        <tr>

          <td><%= tiempo_demorado.cnumero %> - <%= tiempo_demorado.cid %></td>
          <td><%= tiempo_demorado.cfecha_inicio %></td>
          <td><%= tiempo_demorado.cminutos_demorados %></td>
          <td><%= tiempo_demorado.cminutos_estimados %></td>
          <td><%= tiempo_demorado.cminutos %></td>
          <td><%= tiempo_demorado.cfuncionario %></td>

        </tr>
      <% end %>
    </table> 
  <% else %>
    <p class="alert-message warning">
      No hay datos, <%= link_to 'volver', tiempos_demorados_path %>.
    </p>
  <% end %>
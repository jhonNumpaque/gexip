
<table class="table table-striped table-bordered table-condensed">
	<thead>
		<tr>
			<th>Año</th>
			<th>Trimestre</th>
			<th>Proceso</th>
			<th>Cant. Recibidos</th>
			<th>% Personas Físicas</th>
			<th>% Personas Jurídicas</th>
			<th>% Personas Jurídicas Internas</th>
		</tr>
	</thead>
	<tbody>
		<% @porcentaje.each do |key, val| %>
		<% val['anho'].keys.each do |anho| %>
		<% VistaExpedienteTotal::TRIMESTRES.each do |k, v| %>
		<tr>
			<td><%= anho %></td>
			<td><%= link_to v, '#' %></td>
			<td><%= link_to val['nombre'], "#" %></td>
			<td><%= val['anho'][anho]['trimestre'][k].present? ? val['anho'][anho]['trimestre'][k]['cantidad'] : 0 %></td>
			<td><%= val['anho'][anho]['trimestre'][k].present? ? calcular_porcentaje(val['anho'][anho]['trimestre'][k]['cantidad'], val['anho'][anho]['trimestre'][k]['PersonaFisica']) : 0 %></td>
			<td><%= val['anho'][anho]['trimestre'][k].present? ? calcular_porcentaje(val['anho'][anho]['trimestre'][k]['cantidad'], val['anho'][anho]['trimestre'][k]['PersonaJuridica']) : 0 %></td>
			<td><%= val['anho'][anho]['trimestre'][k].present? ? calcular_porcentaje(val['anho'][anho]['trimestre'][k]['cantidad'], val['anho'][anho]['trimestre'][k]['OrganismoInterno']) : 0 %></td>
		</tr>
		<% end %>
		<% end %>
		<% end %>

	</tbody>
</table>